<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude database files from cloud backup -->
        <exclude domain="database" />
    </cloud-backup>
    <device-transfer>
        <!-- Include database files in device transfer -->
        <include domain="database" />
    </device-transfer>
</data-extraction-rules>
